    <main class="container text-center form-signin">
      <% if (db_conn_status == '1') { %>
        <% if (db_response == '0') { %>
          <h2>Έχει ήδη υποβληθεί κριτική για αυτή τη παραγγελια.</h2>
        <% } else { %>
          <h2>Αξιολόγηση εμπειρίας.</h2>
        <% } %>
      <% } else { %>
        <h2>Αποτυχία επικοινωνίας με την βάση δεδομένων μας.</h2>
      <% } %>
      <div class="container d-flex justify-content-center mt-200">
        <form class="" action="/review_deliver" method="POST">
          <div class="d-none">
            <input type="text" name="order_id" value="<%= response.order_id %>">
            <input type="text" name="deliver_id" value="<%= response.deliver_id %>">
            <input type="text" name="user_id" value="<%= response.user_id %>">
            <input type="text" name="user_role" value="<%= response.user_role %>">
          </div>
          <div class="row">
              <div class="col-md-12">
                  <div class="stars">
                      <input class="star star-5" id="star-5" type="radio" name="star" value="five"/> <label class="star star-5" for="star-5"></label>
                      <input class="star star-4" id="star-4" type="radio" name="star" value="four" /> <label class="star star-4" for="star-4"></label>
                      <input class="star star-3" id="star-3" type="radio" name="star" value="three" /> <label class="star star-3" for="star-3"></label> 
                      <input class="star star-2" id="star-2" type="radio" name="star"  value="two" /> <label class="star star-2" for="star-2"></label> 
                      <input class="star star-1" id="star-1" type="radio" name="star" value="one" /> <label class="star star-1" for="star-1"></label>
                  </div>
              </div>
          </div>
          <div class="row">
              <div class="col-12">
                  <label for="review_title">Τίτλος</label>
                  <input class="form-control" id="review_title" name="review_title" required></textarea>
              </div>
          </div>
          <div class="row">
              <div class="col-12">
                  <label for="review_comment">Σχόλια</label>
                  <textarea class="form-control" id="review_comment" name="review_comment"></textarea>
              </div>
          </div>
          <div class="row">
            <div class="col-12 p-3 text-center">
              <div class="form-check">
                <input class="form-check-input" type="radio" name="review_suggested" id="review_suggested_yes" value="yes" checked>
                <label class="form-check-label" for="review_suggested">
                  Ναι
                </label>
              </div>
              <div class="form-check">
                <input class="form-check-input" type="radio" name="review_suggested" id="review_suggested_no" value="no">
                <label class="form-check-label" for="review_suggested">
                  Όχι
                </label>
              </div>
            </div>
          </div>
          <div class="row">
            <div class="col-12">
              <button class="btn btn-primary" type="submit">Υποβολή</button>
            </div>
          </div>
        </form>
      </div>
    </main>
