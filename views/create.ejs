    <main class="container text-center p-4">
      <form action="/create" method="POST">
        <div class="row">
          <h2>Δημιουργία παραγγελίας</h2>
        </div>
        <div class="row">
          <div class="col mb-3">
            <label for="parcelname" class="form-label">Όνομα παραγγελίας</label>
            <input type="text" name="parcelname" class="form-control" id="parcelname" aria-describedby="parcelnameHelp" required>
            <!-- <div id="emailHelp" class="form-text">We'll never share your email with anyone else.</div> -->
          </div>
          <div class="col mb-3">
            <label for="receiversEmail" class="form-label">email παραλήπτη</label>
            <input type="text" name="receiversEmail" class="form-control" id="receiversEmail">
          </div>  
        </div>
        
        <div class="row">
          <div class="col mb-3">
            <label for="sendersAddress" class="form-label">Διεύθυνση παραλαβής</label>
            <input type="text" list="address" name="sendersAddress" class="form-control" id="sendersAddress" aria-describedby="emailHelp">
            <!-- <div id="emailHelp" class="form-text">We'll never share your email with anyone else.</div> -->
            <datalist id="address">
              <% if (address != '-') { %>
              <option value="<%= address %>">
              <% } %>
            </datalist>
          </div>
          <div class="col mb-3">
              <label for="receiversAddress" class="form-label">Διεύθυνση αποστολής</label>
              <input type="text" name="receiversAddress" class="form-control" id="receiversAddress" aria-describedby="emailHelp">
              <!-- <div id="emailHelp" class="form-text">We'll never share your email with anyone else.</div> -->
          </div>
        </div>

        <div class="row">
          <div class="col mb-2">
            <label for="parcelWidth" class="form-label">Πλάτος</label>
            <input type="text" name="parcelWidth" class="form-control" id="parcelWidth" required>
          </div>
          <div class="col mb-4">
            <label for="parcelHeight" class="form-label">Ύψος</label>
            <input type="text" name="parcelHeight" class="form-control" id="parcelHeight" required>
          </div>
          <div class="col mb-6">
            <label for="parcelLength" class="form-label">Μήκος</label>
            <input type="text" name="parcelLength" class="form-control" id="parcelLength" required>
          </div>
          <div class="col mb-8">
            <label for="parcelWeight" class="form-label">Βάρος</label>
            <input type="text" name="parcelWeight" class="form-control" id="parcelWeight" required>
          </div>
          <div class="col mb-10">
            <label for="parcelValue" class="form-label">Εκτιμώμενη αξία</label>
            <input type="text" name="parcelValue" class="form-control" id="parcelValue" required>
          </div>
        </div>

        <div class="row justify-content-center">
          <div class="col-2 mb-3 form-check">
            <input type="checkbox" class="form-check-input" id="exampleCheck1" required>
            <label class="form-check-label" for="exampleCheck1">Συμφωνώ με τους όρους χρήσης</label>
          </div>
        </div>
        <div class="row justify-content-center">
          <button type="submit" class="col-2 btn btn-primary">Υποβολή</button>
        </div>
      </form>
    </main>
    <script>
      const searchbox = document.getElementById('searchbox')
      const searchform = document.getElementById('searchform')

      const width = document.getElementById('parcelWidth')
      const height = document.getElementById('parcelHeight')
      const length = document.getElementById('parcelLength')
      const weigth = document.getElementById('parcelWeight')
      const value = document.getElementById('parcelValue')

      width.onchange = function() {
          console.log("validating if positive");
          let w = width.value;
          if (w < 0) width.setCustomValidity("Μη έγκυρη τιμή")
      };
      height.onchange = validatePositive;
      length.onchange = validatePositive;
      weigth.onchange = validatePositive;
      value.onchange = validatePositive;
      

      console.log(searchbox)
      searchbox.onkeypress = function() {
        console.log(searchbox.value)
      }
      searchform.onsubmit = function() {
        // alert('submiting: ' + (searchbox.value).replace(/\s+/g, ''))
        searchform.action += (searchbox.value).replace(/\s+/g, '')
      }
    </script>
