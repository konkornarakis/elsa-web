    <main class="container text-center">
      <form action="/getjob" method="GET">
        <h3>Ζήτα να αναλάβεις μια μεταφορά, πατώντας το κουμπί</h3>
        <button class="btn btn-primary" type="submit">Ξεκίνα</button>
      </form>
        <% if (db_conn_status === '0') { %>
          <div class="alert alert-warning alert-dismissible fade show" role="alert">
            <strong>Αποτυχία!</strong> Αποτυχία επικοινωνίας με την βάση δεδομένων μας.
            <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
          </div>
        <% } else if (db_response.length == 0) { %>
          <div class="alert alert-warning alert-dismissible fade show m-5" role="alert">
            <strong>Αποτυχία!</strong> Δεν βρήκαμε κάτι
            <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
          </div>
        <% } else { %>
          <h3>Βρήκαμε κάτι. Αναλαμβάνεις την παραγγελία με όνομα: <%= db_response[0].name %>.</h3>
          <table class="table">
            <thead>
              <tr>
                <th scope="col">#</th>
                <th scope="col">Όνομα παραγγελίας</th>
                <th scope="col">Ημ/νία δημιουργίας</th>
                <th scope="col">Κατάσταση</th>
    
                <th scope="col">Ον/νυμο αποστολέα</th>
                <th scope="col">Δ/νση αποστολέα</th>
                <th scope="col">Ον/νυμο παραλήπτη</th>
                <th scope="col">Δ/νση παραλήπτη</th>
              </tr>
            </thead>
            <tbody> 
              <tr>  
                <th scope="row"><%=db_response[0].id%></th>
                <td><%= db_response[0].name %></td>
                <td><%= db_response[0].date_created %></td>
                <td><%= db_response[0].current_status %></td>
                <td><%= db_response[0].sender_name %></td>
                <td><%= db_response[0].sender_address %></td>
                <td><%= db_response[0].receiver_name %></td>
                <td><%= db_response[0].receiver_address %></td>
              </tr>
            </tbody>
          </table>
        <% } %>
